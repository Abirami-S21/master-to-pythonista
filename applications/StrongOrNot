Number = int(input(" Please Enter any Number: "))
Sum = 0
Temp = Number

while (Temp > 0):
    Factorial = 1
    i = 1
    Reminder = Temp % 10

    while (i <= Reminder):
        Factorial = Factorial * i
        i = i + 1
    Sum = Sum + Factorial
    Temp = Temp // 10
if (Sum == Number):
    print(" %d is a Strong Number" % Number)
else:
    print(" %d is not a Strong Number" % Number)
